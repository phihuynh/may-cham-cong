    $(window).scroll(function() {
        if($(window).scrollTop() >= $(".menu").height())
        {
            $("div.menu").css({position:"fixed",left:'0px',right:'0px',top:'0px',zIndex:'999'});
        }
        else
        {
            $("div.menu").css({position:"static"});
        }
    });
</script>
